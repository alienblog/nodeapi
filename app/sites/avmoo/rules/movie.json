{
    "content":{
        "type":"object",
        "fields":{
            "code":{
                "type":"string",
                "selector":".movie p:first span:last",
                "place":"text"
            },
            "title":{
                "type":"string",
                "selector":"h3",
                "place":"text"
            },
            "cover":{
                "type":"string",
                "selector":".movie .bigImage img",
                "place":"attr",
                "param":"href"
            },
            "date":{
                "type":"string",
                "selector":".movie p:nth-child(2)",
                "place":"text",
                "postReplace":[
                    {
                        "origin":"发行时间: ",
                        "newstr":"",
                        "isReg":false
                    }
                ]
            },
            "studioName":{
                "type":"string",
                "selector":".movie p:nth-child(5) a",
                "place":"text"
            },
            "studioId":{
                "type":"string",
                "selector":".movie p:nth-child(5) a",
                "place":"attr",
                "param":"href",
                "postReplace":[
                    {
                        "origin":"https://avmo.pw/cn/studio/",
                        "newstr":"",
                        "isReg":false
                    }
                ]
            },
            "labelName":{
                "type":"string",
                "selector":".movie p:nth-child(7) a",
                "place":"text"
            },
            "labelId":{
                "type":"string",
                "selector":".movie p:nth-child(7) a",
                "place":"attr",
                "param":"href",
                "postReplace":[
                    {
                        "origin":"https://avmo.pw/cn/label/",
                        "newstr":"",
                        "isReg":false
                    }
                ]
            },
            "genres":{
                "type":"array",
                "selector":".movie p:last .genre",
                "content":{
                    "type":"object",
                    "fields":{
                        "name":{
                            "type":"string",
                            "selector":"a",
                            "place":"text"
                        },
                        "genreId":{
                            "type":"string",
                            "selector":"a",
                            "place":"attr",
                            "param":"href",
                            "postReplace":[
                                {
                                    "origin":"https://avmo.pw/cn/genre/",
                                    "newstr":"",
                                    "isReg":false   
                                }
                            ]
                        }
                    }
                }
            },
            "actors":{
                "type":"array",
                "selector":"#avatar-waterfall .avatar-box",
                "content":{
                    "type":"object",
                    "fields":{
                        "actorId":{
                            "type":"string",
                            "selector":"",
                            "place":"attr",
                            "param":"href",
                            "postReplace":[
                                {
                                    "origin":"https://avmo.pw/cn/star/",
                                    "newstr":"",
                                    "isReg":false  
                                }
                            ]
                        },
                        "actorName":{
                            "type":"string",
                            "selector":"span",
                            "place":"text"
                        },
                        "avatar":{
                            "type":"string",
                            "selector":"img",
                            "place":"attr",
                            "param":"src",
                        }
                    }
                }
            },
            "samples":{
                "type":"array",
                "selector":"#sample-waterfall .sample-box",
                "content":{
                    "type":"object",
                    "fields":{
                        "imgUrl":{
                            "type":"string",
                            "selector":"img",
                            "place":"attr",
                            "param":"src",
                        },
                        "imgTitle":{
                            "type":"string",
                            "selector":"img",
                            "place":"attr",
                            "param":"title",
                        }
                    }
                }
            }
        }
    }
}